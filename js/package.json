{
  "name": "pbt",
  "version": "1.0.0",
  "description": "JavaScript code has been tested with [Jasmine](https://jasmine.github.io/). Install the dependencies by [Yarn](https://yarnpkg.com/lang/en/). Just run `yarn`. To run the tests, switch to the `js` folder and run `yarn test`.",
  "main": "node-form.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "qunit": "^2.6.1",
    "sinon": "^5.0.10",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0"
  },
  "scripts": {
    "test": "npx qunit babel-compiled/test/access-creator.tests.js",
    "build": "babel src/**/*.js test/*.js -d babel-compiled --ignore node_modules,webpack-dist,drupal-behavior-function",
    "cleanup-babel": "rm -rf babel-compiled/",
    "build-and-test": "npm run-script cleanup-babel && npm run-script build && npm run-script webpack && npm run-script test",
    "webpack": "npx webpack --entry ./src/**/*.js ./test/*.js --output ./webpack-dist/bundle.js --mode development",
    "bat": "npm run-script build-and-test"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "ignore": [
      "node_modules",
      "package.json",
      "package-json.lock"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
